    //slow the timeline to make it easier
    timeline_speed = 0.5*global.game_speed;
}
else{
    timeline_speed = global.game_speed;
    //give each design pattern a chance to run
    var len, k, v;
    len = ds_map_size(global.seen_enemies);
    k = ds_map_find_first(global.seen_enemies);
    for (var i = 0; i &lt; len; i++;) {
