        var cspr, cx, cy;
        cspr = asset_get_index("eff_cloud_"+string(i));
        cx = room_width;
        cy = irandom_range(-sprite_get_height(cspr), room_height);
        scr_depths_particle_create(cx, cy, part_clouds[i-1]);
