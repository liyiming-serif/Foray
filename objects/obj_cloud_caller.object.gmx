            <kind>1</kind>
            <string>///initialize cloud particles

scr_init_part_cloud();

//place initial clouds on map. A ring of 8 random clouds
for(var p = 0; p &lt; 8; p++){
    var ci, cspr, a, sw, sh, r, cx, cy;
    ci = irandom_range(1,7);
    cspr = asset_get_index("eff_cloud_"+string(ci));
    a = p*45;
    sw = room_width/2;
    sh = room_height/2;
    r = 0.6*(abs(dcos(a)*sw)+abs(dsin(a)*sh));
    cx = sw+lengthdir_x(r, a)-sprite_get_width(cspr)/2;
    cy = sh+lengthdir_y(r, a)-sprite_get_height(cspr)/2;
    scr_depths_particle_create(cx, cy, part_clouds[ci-1]);
} 

//start timer for auto-generating clouds
alarm[0] = 240; //t = tiling_dist/cloud_speed
</string>
